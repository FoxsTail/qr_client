        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.lis.qr_client.activity.TestActivity">

    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:orientation="vertical"
                                          android:id="@+id/guideline" app:layout_constraintGuide_percent="0.79"
                                          tools:layout_editor_absoluteY="0dp" tools:layout_editor_absoluteX="304dp"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:orientation="vertical"
                                          android:id="@+id/guideline2" app:layout_constraintGuide_percent="0.9557292"
                                          tools:layout_editor_absoluteY="0dp" tools:layout_editor_absoluteX="367dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:id="@+id/button3"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="299dp" app:layout_constraintLeft_toLeftOf="@+id/button5"
            android:layout_marginLeft="-127dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button4"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="299dp"
            app:layout_constraintLeft_toLeftOf="@+id/button3" android:layout_marginLeft="65dp"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline4" app:layout_constraintGuide_begin="53dp"
                                          android:orientation="horizontal" tools:layout_editor_absoluteY="53dp"
                                          tools:layout_editor_absoluteX="0dp"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button5" tools:layout_editor_absoluteY="299dp"
            app:layout_constraintLeft_toRightOf="@+id/button3" android:layout_marginLeft="8dp"
            app:layout_constraintRight_toLeftOf="@+id/button4" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.666" android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>


</android.support.constraint.ConstraintLayout>
